<configuration>
	<system.webServer>
		<handlers>
			<add name="iisnode" path="app.js" verb="*" modules="iisnode" />
		</handlers>    
		
		<iisnode 
			promoteServerVars="LOGON_USER,APPL_MD_PATH" 
			enableXFF="true" 
			devErrorsEnabled="false"
		/>
		
		
		<rewrite>
			<rules>
				<rule name="app">
					<match url="/*" />
					<action type="Rewrite" url="app.js" />
				</rule>
			</rules>
		</rewrite>
	</system.webServer>
	
    <system.web>
        <identity impersonate="false" />
    </system.web>
</configuration>

